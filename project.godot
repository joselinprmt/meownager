config_version=5

[application]

config/name="Meownager"
run/main_scene="res://scenes/ui/menu_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[autoload]

ToolManager="*res://scripts/globals/tool_manager.gd"
InventoryManager="*res://scripts/globals/inventory_manager.gd"
DayCycleManager="*res://scripts/globals/day_cycle_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
GameManager="*res://scripts/globals/game_manager.gd"
BackgroundMusic="*res://scenes/audio/background_music.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Tilesets/": "pink",
"res://assets/": "teal",
"res://dialogue/": "yellow",
"res://scenes/": "blue",
"res://scripts/": "purple"
}

